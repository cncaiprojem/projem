{
  "title": "FreeCAD Platformu - İzleme",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "İş Gecikmesi (s)",
      "fieldConfig": {"defaults": {"unit": "s"}},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(job_latency_seconds_bucket[5m])) by (le,type,status))", "legendFormat": "p95 {{type}} {{status}}"},
        {"expr": "histogram_quantile(0.50, sum(rate(job_latency_seconds_bucket[5m])) by (le,type,status))", "legendFormat": "p50 {{type}} {{status}}"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Kuyruk Bekleme (s)",
      "fieldConfig": {"defaults": {"unit": "s"}},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(queue_wait_seconds_bucket[5m])) by (le,queue))", "legendFormat": "p95 {{queue}}"},
        {"expr": "histogram_quantile(0.50, sum(rate(queue_wait_seconds_bucket[5m])) by (le,queue))", "legendFormat": "p50 {{queue}}"}
      ]
    },
    {
      "type": "stat",
      "title": "Hata Sayıları (son 5m)",
      "targets": [
        {"expr": "sum(increase(failures_total[5m])) by (task,reason)", "legendFormat": "{{task}} {{reason}}"}
      ]
    },
    {
      "type": "stat",
      "title": "Retry Sayıları (son 5m)",
      "targets": [
        {"expr": "sum(increase(retried_total[5m])) by (task)", "legendFormat": "{{task}}"}
      ]
    }
  ],
  "templating": {"list": []},
  "time": {"from": "now-6h", "to": "now"}
}

